<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC
        "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.xy.community.dao.UserDao">
    <insert id="insert">
        insert into user(name,account_id,token,gmt_create,gmt_modified) values(#{name},#{accountId},#{token},#{gmtCreate},#{gmtModified})
    </insert>
    <select id="findByToken" resultType="com.xy.community.model.User">
        select * from user where token=#{token}
    </select>
</mapper>

